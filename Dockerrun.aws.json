{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "image": "andrewwallace/cobweb_web:latest",
            "memory": 128,
            "command": [
                "python3",
                "manage.py",
                "runserver",
                "0.0.0.0:8000"
            ],
            "essential": true,
            "mountPoints": [
                {
                    "containerPath": "/code",
                    "sourceVolume": "django-app"
                }
            ],
            "name": "web",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 80
                }
            ]
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "/var/app/current/app/"
            },
            "name": "django-app"
        }
    ]
}
